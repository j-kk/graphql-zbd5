type AdDimensions {
  width: Int!
  height: Int!
}

type Ad {
  id: Int!
  dimensions: AdDimensions
  texts: [String!]
  mainColor: String
}

input NewAdDimensions {
  width: Int!
  height: Int!
}

input NewAd {
  dimensions: NewAdDimensions
  texts: [String!]
  mainColor: String
}

type Mutation {
  registerAd(input: NewAd!): Ad!
}
